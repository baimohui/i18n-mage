            "order": 22,
            "type": "integer",
            "default": 40,
            "minimum": 10,
            "description": "%config.maxGeneratedKeyLength.desc%"
          },
          "i18n-mage.writeRules.languageFileIndent": {
            "order": 24,
            "type": [
              "integer",
              "null"
            ],
            "default": null,
            "minimum": 0,
            "maximum": 8,
            "description": "%config.languageFileIndent.desc%"
