{
  "name": "i18n-checker",
  "displayName": "i18n Mage",
  "description": "Detect, repair and sort i18n",
  "repository": {
    "type": "git",
    "url": "https://github.com/baimohui/i18n-checker.git"
  },
  "publisher": "jensenwen",
  "keywords": [
    "i18n",
    "translate",
    "language"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "configuration": [
      {
        "type": "object",
        "title": "%config.common.title%",
        "properties": {
          "i18n-mage.referenceLanguage": {
            "type": "string",
            "default": "cn",
            "description": "%config.referenceLanguage.desc%"
          },
          "i18n-mage.previewBeforeFix": {
            "type": "boolean",
            "default": true,
            "description": "%config.previewBeforeFix.desc%"
          },
          "i18n-mage.syncBasedOnReferredEntries": {
            "type": "boolean",
            "default": false,
            "description": "%config.syncBasedOnReferredEntries.desc%"
          },
          "i18n-mage.ignoreEmptyLangFile": {
            "type": "boolean",
            "default": true,
            "description": "%config.ignoreEmptyLangFile.desc%"
          },
          "i18n-mage.langFileMinLength": {
            "type": "integer",
            "default": 30,
            "description": "%config.langFileMinLength.desc%"
          },
          "i18n-mage.ignoredFileList": {
            "type": "array",
            "default": [],
            "description": "%config.ignoredFileList.desc%"
          }
        }
      },
      {
        "type": "object",
        "title": "%config.translatePlatform.title%",
        "properties": {
          "i18n-mage.baiduAppId": {
            "type": "string",
            "default": "",
            "description": "%config.baiduAppId.desc%"
          },
          "i18n-mage.baiduSecretKey": {
            "type": "string",
            "default": "",
            "description": "%config.baiduSecretKey.desc%"
          },
          "i18n-mage.tencentSecretId": {
            "type": "string",
            "default": "",
            "description": "%config.tencentSecretId.desc%"
          },
          "i18n-mage.tencentSecretKey": {
            "type": "string",
            "default": "",
            "description": "%config.tencentSecretKey.desc%"
          },
          "i18n-mage.translateApiPriority": {
            "type": "array",
            "default": [
              "google",
              "baidu",
              "tencent"
            ],
            "description": "%config.translateApiPriority.desc%"
          },
          "i18n-mage.langAliasCustomMappings": {
            "type": "object",
            "description": "%config.langAliasCustomMappings.desc%",
            "default": {},
            "additionalProperties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      }
    ],
    "commands": [
      {
        "command": "i18nMage.setReferredLang",
        "title": "%command.setReferredLang.title%"
      },
      {
        "command": "i18nMage.markAsKnownLang",
        "title": "%command.markAsKnownLang.title%",
        "icon": {
          "light": "./images/claim-light.svg",
          "dark": "./images/claim-dark.svg"
        }
      },
      {
        "command": "i18nMage.checkUsage",
        "title": "%command.refresh.title%",
        "icon": {
          "light": "./images/refresh-light.svg",
          "dark": "./images/refresh-dark.svg"
        }
      },
      {
        "command": "i18nMage.fix",
        "title": "%command.fix.title%",
        "icon": {
          "light": "./images/translate-light.svg",
          "dark": "./images/translate-dark.svg"
        }
      },
      {
        "command": "i18nMage.export",
        "title": "%command.export.title%",
        "icon": {
          "light": "./images/share-light.svg",
          "dark": "./images/share-dark.svg"
        }
      },
      {
        "command": "i18nMage.import",
        "title": "%command.import.title%",
        "icon": {
          "light": "./images/import-light.svg",
          "dark": "./images/import-dark.svg"
        }
      },
      {
        "command": "i18nMage.ignoreFile",
        "title": "%command.ignoreFile.title%",
        "icon": {
          "light": "./images/remove-light.svg",
          "dark": "./images/remove-dark.svg"
        }
      },
      {
        "command": "i18nMage.copyName",
        "title": "%command.copyName.title%",
        "icon": {
          "light": "./images/copy-light.svg",
          "dark": "./images/copy-dark.svg"
        }
      },
      {
        "command": "i18nMage.copyKeyValue",
        "title": "%command.copyKeyValue.title%",
        "icon": {
          "light": "./images/copy-light.svg",
          "dark": "./images/copy-dark.svg"
        }
      },
      {
        "command": "i18nMage.goToReference",
        "title": "%command.goToReference.title%",
        "icon": {
          "light": "./images/source-light.svg",
          "dark": "./images/source-dark.svg"
        }
      },
      {
        "command": "i18nMage.goToDefinition",
        "title": "%command.goToDefinition.title%",
        "icon": {
          "light": "./images/definition-light.svg",
          "dark": "./images/definition-dark.svg"
        }
      },
      {
        "command": "i18nMage.copyValue",
        "title": "%command.copyValue.title%",
        "icon": {
          "light": "./images/copy-light.svg",
          "dark": "./images/copy-dark.svg"
        }
      },
      {
        "command": "i18nMage.editValue",
        "title": "%command.editValue.title%",
        "icon": {
          "light": "./images/edit-light.svg",
          "dark": "./images/edit-dark.svg"
        }
      },
      {
        "command": "i18nMage.deleteUnused",
        "title": "%command.deleteUnused.title%",
        "icon": {
          "light": "./images/delete-light.svg",
          "dark": "./images/delete-dark.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "i18nMage.checkUsage",
          "when": "view == treeProvider && hasValidLangDir",
          "group": "navigation@1"
        },
        {
          "command": "i18nMage.export",
          "when": "view == treeProvider && hasValidLangDir",
          "group": "navigation@2"
        },
        {
          "command": "i18nMage.import",
          "when": "view == treeProvider && hasValidLangDir",
          "group": "navigation@3"
        },
        {
          "command": "i18nMage.fix",
          "when": "view == treeProvider && hasValidLangDir",
          "group": "navigation@4"
        }
      ],
      "view/item/context": [
        {
          "command": "i18nMage.setReferredLang",
          "when": "viewItem =~ /checkSyncInfo/"
        },
        {
          "command": "i18nMage.markAsKnownLang",
          "when": "viewItem =~ /INVALID_LANG/",
          "group": "inline@1"
        },
        {
          "command": "i18nMage.checkUsage",
          "when": "viewItem == checkUsage",
          "group": "inline"
        },
        {
          "command": "i18nMage.ignoreFile",
          "when": "viewItem == fileItem"
        },
        {
          "command": "i18nMage.copyName",
          "when": "viewItem =~ /COPY_NAME/",
          "group": "inline@1"
        },
        {
          "command": "i18nMage.copyKeyValue",
          "when": "viewItem =~ /COPY_KEY_VALUE_LIST/"
        },
        {
          "command": "i18nMage.goToReference",
          "when": "viewItem =~ /definedEntryInCurFile|undefinedEntryInCurFile/",
          "group": "inline@2"
        },
        {
          "command": "i18nMage.goToDefinition",
          "when": "viewItem =~ /GO_TO_DEFINITION/",
          "group": "inline@2"
        },
        {
          "command": "i18nMage.copyValue",
          "when": "viewItem =~ /COPY_VALUE/",
          "group": "inline@1"
        },
        {
          "command": "i18nMage.editValue",
          "when": "viewItem =~ /EDIT_VALUE/",
          "group": "inline@3"
        },
        {
          "command": "i18nMage.deleteUnused",
          "when": "viewItem == unusedGroupHeader || viewItem == unusedGroupItem",
          "group": "inline@1"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "i18nMage",
          "title": "i18n Mage",
          "icon": "images/i18nMage.png"
        }
      ]
    },
    "views": {
      "i18nMage": [
        {
          "icon": "images/i18nMage.png",
          "id": "treeProvider",
          "name": "",
          "when": "workspaceFolderCount != 0"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "treeProvider",
        "contents": "%view.welcome.title%",
        "when": "initialized  && !hasValidLangDir"
      }
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "build": "node esbuild.config.js",
    "package": "npm run build && vsce package",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.9.0",
    "js-md5": "^0.7.3",
    "json5": "^2.2.3",
    "node-xlsx": "^0.24.0",
    "tencentcloud-sdk-nodejs-tmt": "^4.0.791",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/preset-env": "^7.24.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.97",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "esbuild": "^0.25.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}