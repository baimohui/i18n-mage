              "double",
              "single",
              "none"
            ],
            "default": "auto",
            "enumDescriptions": [
              "%config.quoteStyle.auto%",
              "%config.quoteStyle.double%",
              "%config.quoteStyle.single%",
              "%config.quoteStyle.none%"
            ],
            "description": "%config.quoteStyleForKey.desc%"
