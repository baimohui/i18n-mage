{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\n",
  "git.ignoreRebaseWarning": true,
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "i18n-mage.i18nFeatures.framework": "vscode-l10n",
  "i18n-mage.translationServices.referenceLanguage": "zh-CN",
  "i18n-mage.writeRules.sortRule": "byKey",
  "i18n-mage.writeRules.sortAfterFix": true,
  "i18n-mage.general.displayLanguage": "zh-CN",
  "i18n-mage.workspace.languagePath": "src/locales",
  "i18n-mage.workspace.ignoredDirectories": [
    "test",
    "dist"
  ],
  "i18n-mage.analysis.fileExtensions": [
    ".ts",
    ".tsx"
  ],
  "i18n-mage.translationHints.enable": true,
  "i18n-mage.writeRules.indentType": "space",
  "i18n-mage.writeRules.indentSize": 2,
  "i18n-mage.writeRules.languageStructure": "flat",
  "i18n-mage.writeRules.quoteStyleForKey": "double",
  "i18n-mage.writeRules.quoteStyleForValue": "double",
  "i18n-mage.workspace.extractScopeBlacklist": [
    "src/webviews",
    "src/utils/langKey/constants.ts",
    "src/translator/deepseek.ts",
    "vite.config.ts"
  ],
  "i18n-mage.extract.jsTsImportLines": [
    "import { t } from \"@/utils/i18n\";"
  ],
  "i18n-mage.extract.ignoreTexts": [
    "@ext:jensen-wen.i18n-mage",
    "i18n Mage",
    "[[[SEP]]]",
    "sync~spin"
  ]
}