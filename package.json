              "deepl",
              "deepseek",
              "baidu",
              "tencent",
              "google"
            ],
            "description": "%config.translateApiPriority.desc%",
            "items": {
              "type": "string",
              "enum": [
                "deepl",
                "deepseek",
                "google",
                "baidu",
                "tencent"
              ]
            },
            "uniqueItems": true
          },
          "i18n-mage.translationServices.deeplVersion": {
            "order": 18,
            "type": "string",
