            "default": "query",
            "description": "%config.unmatchedLanguageAction.desc%",
            "enumDescriptions": [
              "%config.unmatchedLanguageAction.ignore%",
              "%config.unmatchedLanguageAction.force%",
              "%config.unmatchedLanguageAction.fill%",
              "%config.unmatchedLanguageAction.switch%",
              "%config.unmatchedLanguageAction.query%"
