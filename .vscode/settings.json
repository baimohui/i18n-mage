{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\n",
  "git.ignoreRebaseWarning": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-mage.langPath": "l10n",
  "i18n-mage.ignoredFileList": [
    "test/unit/utils/regex.test.ts"
  ],
  "i18n-mage.i18nSolution": "vscode-l10n",
  "i18n-mage.referenceLanguage": "bundle.l10n.zh-cn",
  "i18n-mage.langAliasCustomMappings": {
    "en": [
      "bundle.l10n"
    ],
    "zh-CN": [
      "bundle.l10n.zh-cn"
    ]
  },
  "i18n-mage.sorting.writeMode": "byKey"
}