{
  "name": "i18n-mage",
  "displayName": "i18n Mage",
  "description": "ðŸª„ Wave your i18n wand. Conjure order from the chaos with auto-translation, smart fixes, and Excel sync.",
  "repository": {
    "type": "git",
    "url": "https://github.com/baimohui/i18n-mage.git"
  },
  "publisher": "jensen-wen",
  "keywords": [
    "i18n",
    "translation",
    "localization",
    "internationalization",
    "multilingual",
    "vue",
    "vue-i18n",
    "react",
    "react-i18next",
    "nextjs",
    "ç¿»è¯‘",
    "å›½é™…åŒ–"
  ],
  "version": "1.0.7",
  "engines": {
    "vscode": "^1.89.0"
  },
  "icon": "images/icon.png",
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [
    "workspaceContains:package.json",
    "onCommand:i18nMage.*",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:json",
    "onLanguage:vue"
  ],
  "main": "dist/extension.js",
  "contributes": {
    "configuration": [
      {
        "type": "object",
        "title": "%config.general.title%",
        "properties": {
          "i18n-mage.general.enable": {
            "order": 0,
            "type": "boolean",
            "default": true,
            "description": "%config.enable.desc%"
          },
          "i18n-mage.general.previewChanges": {
            "order": 4,
            "type": "boolean",
            "default": true,
            "description": "%config.previewChanges.desc%"
          },
          "i18n-mage.general.enableDiagnostics": {
            "order": 6,
            "type": "boolean",
            "default": true,
            "description": "%config.enableDiagnostics.desc%"
          },
          "i18n-mage.general.displayLanguage": {
            "order": 8,
            "type": "string",
            "default": "",
            "description": "%config.displayLanguage.desc%"
          },
          "i18n-mage.general.fileExtensions": {
            "order": 12,
            "type": "array",
            "default": [
              ".js",
              ".ts",
              ".jsx",
              ".tsx",
              ".vue",
              ".html",
              ".json"
            ],
            "description": "%config.fileExtensions.desc%",
            "items": {
              "type": "string"
            }
          },
          "i18n-mage.general.sortOnExport": {
            "order": 16,
            "type": "string",
            "enum": [
              "none",
              "byKey",
              "byPosition"
            ],
            "default": "byKey",
            "enumDescriptions": [
              "%config.sortBy.none%",
              "%config.sortBy.byKey%",
              "%config.sortBy.byPosition%"
            ],
            "description": "%config.sorting.exportMode%"
          }
        }
      },
      {
        "type": "object",
        "title": "%config.i18nFeatures.title%",
        "properties": {
          "i18n-mage.i18nFeatures.framework": {
            "order": 1,
            "type": "string",
            "default": "none",
            "description": "%config.i18nFramework.desc%",
            "enum": [
              "none",
              "vue-i18n",
              "react-i18next",
              "i18next",
              "vscode-l10n"
            ]
          },
          "i18n-mage.i18nFeatures.translationFunctionNames": {
            "order": 5,
            "type": "array",
            "default": [
              "t"
            ],
            "description": "%config.translationFunctionNames.desc%",
            "items": {
              "type": "string"
            }
          },
          "i18n-mage.i18nFeatures.namespaceStrategy": {
            "order": 7,
            "enum": [
              "auto",
              "full",
              "file",
              "none"
            ],
            "enumDescriptions": [
              "%config.namespaceStrategy.auto%",
              "%config.namespaceStrategy.full%",
              "%config.namespaceStrategy.file%",
              "%config.namespaceStrategy.none%"
            ],
            "default": "auto",
            "description": "%config.namespaceStrategy.desc%"
          },
          "i18n-mage.i18nFeatures.interpolationBrackets": {
            "order": 8,
            "type": "string",
            "default": "auto",
            "enum": [
              "auto",
              "single",
              "double"
            ],
            "enumDescriptions": [
              "%config.i18nFeatures.auto%",
              "%config.i18nFeatures.singleBrackets%",
              "%config.i18nFeatures.doubleBrackets%"
            ],
            "description": "%config.interpolationBrackets.desc%"
          },
          "i18n-mage.i18nFeatures.namespaceSeparator": {
            "order": 10,
            "type": "string",
            "enum": [
              "auto",
              ":",
              "."
            ],
            "enumDescriptions": [
              "%config.i18nFeatures.auto%"
            ],
            "default": "auto",
            "description": "%config.namespaceSeparator.desc%"
          },
          "i18n-mage.i18nFeatures.defaultNamespace": {
            "order": 12,
            "type": "string",
            "default": "translation",
            "description": "%config.defaultNamespace.desc%"
          }
        }
      },
      {
        "type": "object",
        "title": "%config.translateServices.title%",
