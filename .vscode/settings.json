{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact"
  ],
  "files.eol": "\n",
  "git.ignoreRebaseWarning": true,
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "i18n-mage.i18nFeatures.framework": "vscode-l10n",
  "i18n-mage.translationServices.referenceLanguage": "bundle.l10n.zh-cn",
  "i18n-mage.translationServices.langAliasCustomMappings": {
    "en": [
      "bundle.l10n"
    ],
    "zh-CN": [
      "bundle.l10n.zh-cn"
    ]
  },
  "i18n-mage.writeRules.sortRule": "byKey",
  "i18n-mage.writeRules.sortAfterFix": true,
  "i18n-mage.general.displayLanguage": "bundle.l10n.zh-cn",
  "i18n-mage.workspace.languagePath": "l10n",
  "i18n-mage.workspace.ignoredDirectories": [
    "test"
  ]
}