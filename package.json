          "dark": "./images/copy-dark.svg"
        }
      },
      {
        "command": "i18nMage.copyKeyValue",
        "category": "%extension.name%",
        "title": "%command.copyKeyValue.title%",
        "icon": {
          "light": "./images/copy-light.svg",
          "dark": "./images/copy-dark.svg"
        }
      },
      {
        "command": "i18nMage.goToReference",
        "category": "%extension.name%",
        "title": "%command.goToReference.title%",
        "icon": {
          "light": "./images/source-light.svg",
          "dark": "./images/source-dark.svg"
        }
      },
      {
        "command": "i18nMage.goToDefinition",
        "category": "%extension.name%",
        "title": "%command.goToDefinition.title%",
        "icon": {
          "light": "./images/definition-light.svg",
          "dark": "./images/definition-dark.svg"
        }
      },
      {
        "command": "i18nMage.copyValue",
        "category": "%extension.name%",
        "title": "%command.copyValue.title%",
        "icon": {
          "light": "./images/copy-light.svg",
          "dark": "./images/copy-dark.svg"
        }
      },
      {
        "command": "i18nMage.editValue",
        "category": "%extension.name%",
        "title": "%command.editValue.title%",
        "icon": {
          "light": "./images/edit-light.svg",
          "dark": "./images/edit-dark.svg"
        }
      },
      {
        "command": "i18nMage.deleteUnused",
        "category": "%extension.name%",
        "title": "%command.deleteUnused.title%",
        "icon": {
          "light": "./images/delete-light.svg",
          "dark": "./images/delete-dark.svg"
        }
      },
      {
        "command": "i18nMage.markAsUsed",
        "category": "%extension.name%",
        "title": "%command.markAsUsed.title%",
        "icon": {
          "light": "./images/pin-light.svg",
          "dark": "./images/pin-dark.svg"
        }
      },
      {
        "command": "i18nMage.unmarkAsUsed",
        "category": "%extension.name%",
        "title": "%command.unmarkAsUsed.title%",
        "icon": {
          "light": "./images/pinFill-light.svg",
          "dark": "./images/pinFill-dark.svg"
        }
      },
      {
        "command": "i18nMage.ignoreUndefined",
        "category": "%extension.name%",
        "title": "%command.ignoreUndefined.title%",
        "icon": {
          "light": "./images/ignore-light.svg",
          "dark": "./images/ignore-dark.svg"
        }
      },
      {
        "command": "i18nMage.setProjectPath",
        "category": "%extension.name%",
        "title": "%command.setProjectPath.title%"
      },
      {
        "command": "i18nMage.setLangPath",
        "category": "%extension.name%",
        "title": "%command.setLangPath.title%"
      },
      {
        "command": "i18nMage.toggleInlineTranslation",
        "category": "%extension.name%",
        "title": "%command.toggleInlineTranslation.title%"
      }
    ],
    "keybindings": [
      {
        "command": "i18nMage.toggleInlineTranslation",
        "key": "ctrl+alt+d",
        "when": "enabled && hasValidLangPath && editorTextFocus"
      },
      {
        "command": "i18nMage.checkUsage",
        "key": "ctrl+alt+c",
        "when": "workspaceFolderCount != 0 && enabled"
      },
      {
        "command": "i18nMage.fix",
        "key": "ctrl+alt+f",
        "when": "enabled && hasValidLangPath"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "i18nMage.checkUsage",
          "when": "view == treeProvider && hasValidLangPath",
          "group": "navigation@5"
        },
        {
          "command": "i18nMage.export",
          "when": "view == treeProvider && hasValidLangPath",
          "group": "navigation@10"
        },
        {
          "command": "i18nMage.import",
          "when": "view == treeProvider && hasValidLangPath",
          "group": "navigation@15"
        },
        {
          "command": "i18nMage.sort",
          "when": "view == treeProvider && hasValidLangPath && allowSort",
          "group": "navigation@20"
        },
        {
          "command": "i18nMage.fix",
          "when": "view == treeProvider && hasValidLangPath",
          "group": "navigation@25"
        },
        {
          "command": "i18nMage.openDocs",
          "when": "view == treeProvider",
          "group": "view@1"
        },
        {
          "command": "i18nMage.openSettings",
          "when": "view == treeProvider",
          "group": "view@2"
        },
        {
          "command": "i18nMage.feedback",
          "when": "view == treeProvider",
          "group": "view@3"
        }
      ],
      "view/item/context": [
        {
          "command": "i18nMage.setReferredLang",
          "when": "viewItem =~ /checkSyncInfo/ && !(viewItem =~ /UNKNOWN_LANG/) && !(viewItem =~ /REFERENCE_LANG/) && !(viewItem =~ /IGNORED_LANG/)",
          "group": "syncInfo@1"
        },
        {
          "command": "i18nMage.setDisplayLang",
          "when": "viewItem =~ /checkSyncInfo/ && !(viewItem =~ /DISPLAY_LANG/) && !(viewItem =~ /IGNORED_LANG/)",
          "group": "syncInfo@2"
        },
        {
          "command": "i18nMage.ignoreLang",
          "when": "viewItem =~ /checkSyncInfo/ && !(viewItem =~ /DISPLAY_LANG/) && !(viewItem =~ /REFERENCE_LANG/) && !(viewItem =~ /IGNORED_LANG/)",
          "group": "syncInfo@3"
        },
        {
          "command": "i18nMage.unignoreLang",
          "when": "viewItem =~ /checkSyncInfo/ && viewItem =~ /IGNORED_LANG/",
          "group": "syncInfo@4"
        },
        {
          "command": "i18nMage.markAsKnownLang",
          "when": "viewItem =~ /UNKNOWN_LANG/",
          "group": "inline@1"
        },
        {
          "command": "i18nMage.ignoreFile",
          "when": "viewItem == fileItem"
        },
        {
          "command": "i18nMage.copyName",
          "when": "viewItem =~ /COPY_NAME/",
          "group": "inline@1"
        },
        {
          "command": "i18nMage.copyKeyValue",
          "when": "viewItem =~ /COPY_KEY_VALUE_LIST/"
        },
        {
          "command": "i18nMage.goToReference",
          "when": "viewItem =~ /definedEntryInCurFile|undefinedEntryInCurFile/",
          "group": "inline@2"
        },
        {
          "command": "i18nMage.goToDefinition",
          "when": "viewItem =~ /GO_TO_DEFINITION/",
          "group": "inline@2"
        },
        {
          "command": "i18nMage.copyValue",
          "when": "viewItem =~ /COPY_VALUE/",
          "group": "inline@1"
        },
        {
          "command": "i18nMage.editValue",
          "when": "viewItem =~ /EDIT_VALUE/",
          "group": "inline@3"
        },
        {
          "command": "i18nMage.markAsUsed",
          "when": "viewItem == unusedGroupHeader || viewItem =~ /unusedGroupItem/",
          "group": "inline@2"
        },
        {
          "command": "i18nMage.deleteUnused",
          "when": "viewItem == unusedGroupHeader || viewItem =~ /unusedGroupItem/",
          "group": "inline@3"
        },
        {
          "command": "i18nMage.unmarkAsUsed",
          "when": "viewItem == usedGroupItem-None",
          "group": "inline@1"
        },
        {
          "command": "i18nMage.ignoreUndefined",
          "when": "viewItem == undefinedGroupHeader || viewItem =~ /IGNORE_UNDEFINED/",
          "group": "inline@3"
        },
        {
          "command": "i18nMage.fixUndefinedEntries",
          "when": "viewItem =~ /GEN_KEY/",
          "group": "inline@5"
        },
        {
          "command": "i18nMage.fillMissingTranslations",
          "when": "viewItem =~ /FILL_VALUE/",
          "group": "inline@5"
        }
      ],
      "editor/context": [
        {
          "command": "i18nMage.copyKeyValue",
          "when": "hasDefinedEntriesInFile",
          "group": "2_i18n@1"
        },
        {
          "command": "i18nMage.fixUndefinedEntries",
          "when": "hasUndefinedEntriesInFile",
          "group": "2_i18n@3"
        }
      ],
      "explorer/context": [
        {
          "submenu": "i18nMage.pathConfigMenu",
          "when": "enabled",
          "group": "3_i18n@1"
        }
      ],
      "i18nMage.pathConfigMenu": [
        {
          "command": "i18nMage.setLangPath",
          "when": "explorerResourceIsFolder",
          "group": "navigation@4"
        },
        {
          "command": "i18nMage.setProjectPath",
          "when": "explorerResourceIsFolder",
          "group": "navigation@6"
        },
        {
          "command": "i18nMage.ignoreDirectory",
          "when": "explorerResourceIsFolder",
          "group": "navigation@8"
        }
      ]
    },
    "submenus": [
      {
        "id": "i18nMage.pathConfigMenu",
        "label": "%extension.submenu.pathConfig%"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "i18nMage",
          "title": "%extension.name%",
          "icon": "images/i18nMage.png"
        }
      ]
    },
    "views": {
      "i18nMage": [
        {
          "icon": "images/i18nMage.png",
          "id": "treeProvider",
          "name": "Grimoire",
          "when": "initialized && workspaceFolderCount != 0 && enabled"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "treeProvider",
        "contents": "%view.welcome.title%",
        "when": "initialized  && !hasValidLangPath"
      }
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "npm run lint:code && npm run lint:test",
    "lint:code": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:test": "eslint \"test/**/*.{ts,tsx}\" --no-fix",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check",
    "test:unit": "mocha --require ts-node/register --require tsconfig-paths/register ./test/setup.ts \"test/**/*.ts\"",
    "test:integration": "vscode-test --extensionDevelopmentPath=. --extensionTestsPath=test/integration/index.ts",
    "build": "node esbuild.config.js",
    "package": "npm run build && vsce package",
    "format": "prettier --write \"*.{ts,tsx,js,jsx,md}\""
  },
  "mocha": {
    "timeout": 2000
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.12.0",
    "js-md5": "^0.7.3",
    "json5": "^2.2.3",
    "node-xlsx": "^0.24.0",
    "tencentcloud-sdk-nodejs-tmt": "^4.0.791",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/preset-env": "^7.24.8",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/mock-require": "^3.0.0",
    "@types/node": "^18.19.97",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.5.2",
    "chai": "^5.2.0",
    "esbuild": "^0.25.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "mocha": "^11.5.0",
    "mock-require": "^3.0.3",
    "prettier": "^3.5.3",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}