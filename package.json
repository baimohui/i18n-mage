          "i18n-mage.translationServices.autoTranslateEmptyKey": {
            "order": 9,
            "type": "boolean",
            "default": true,
            "description": "%config.autoTranslateEmptyKey.desc%"
          },
          "i18n-mage.translationServices.referenceLanguage": {
            "order": 10,
            "type": "string",
            "default": "",
            "description": "%config.referenceLanguage.desc%"
          },
          "i18n-mage.translationServices.langAliasCustomMappings": {
            "order": 12,
            "type": "object",
            "description": "%config.langAliasCustomMappings.desc%",
            "default": {},
            "additionalProperties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          "i18n-mage.translationServices.translateApiPriority": {
            "order": 14,
