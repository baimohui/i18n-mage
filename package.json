          },
          "i18n-mage.translationServices.deeplApiKey": {
            "order": 19,
            "type": "string",
            "default": "",
            "description": "%config.deeplApiKey.desc%"
          },
          "i18n-mage.translationServices.deepseekApiKey": {
            "order": 25,
            "type": "string",
            "default": "",
            "description": "%config.deepseekApiKey.desc%"
          },
          "i18n-mage.translationServices.baiduAppId": {
            "order": 27,
            "type": "string",
            "default": "",
            "description": "%config.baiduAppId.desc%"
          },
          "i18n-mage.translationServices.baiduSecretKey": {
            "order": 28,
            "type": "string",
            "default": "",
            "description": "%config.baiduSecretKey.desc%"
          },
          "i18n-mage.translationServices.tencentSecretId": {
            "order": 31,
            "type": "string",
            "default": "",
            "description": "%config.tencentSecretId.desc%"
          },
          "i18n-mage.translationServices.tencentSecretKey": {
            "order": 32,
            "type": "string",
            "default": "",
            "description": "%config.tencentSecretKey.desc%"
          },
          "i18n-mage.translationServices.proxy.enable": {
            "order": 35,
            "type": "boolean",
            "default": false,
            "description": "%config.enableProxy.desc%"
