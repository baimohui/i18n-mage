            "enumDescriptions": [
              "%config.generatedKeyStyle.camelCase%",
              "%config.generatedKeyStyle.pascalCase%",
              "%config.generatedKeyStyle.snakeCase%",
              "%config.generatedKeyStyle.kebabCase%",
              "%config.generatedKeyStyle.raw%"
